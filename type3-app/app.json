{
  "expo": {
    "name": "type3-app",
    "slug": "type3-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tzvigr.type3app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to provide better services.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location even when it's in the background."
      }

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "eb86e60e-750d-42de-8a8a-f1d05519a145"
      }
    }
  }
}
